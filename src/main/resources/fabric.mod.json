{
	"schemaVersion": 1,
	"id": "neruina",
	"version": "${version}",
	"name": "Neruina",
	"description": "A mod that prevents ticking related crashes from bricking worlds",
	"authors": [
		"Bawnorton"
	],
	"contact": {
		"homepage": "https://bawnorton.com",
		"sources": "https://github.com/Benjamin-Norton/Neruina"
	},
	"license": "CC0-1.0",
	"icon": "assets/neruina/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.bawnorton.neruina.Neruina"
		],
		"client": [
			"com.bawnorton.neruina.NeruinaClient"
		]
	},
	"mixins": [
		"neruina.mixins.json",
